<div class="span3">
  <div class="well sidebar-nav">
    <ul class="nav nav-list">
        <li class="nav-header">
        % if (exists $navbar->{name}) {
            <%=$navbar->{name}%>
        % } else {
            <%=$action%>
        % };
        </li>
        % foreach my $li (sort keys %{$navbar}) {
            % if ($li ne 'name') {
        <li><a href="<%= $navbar->{$li} %>"><%= $li %></a></li>
            % };
        % };
    </ul>
  </div><!--/.well -->
</div><!--/span-->
